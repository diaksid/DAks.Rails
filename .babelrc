{
  "presets": [
    "es2015",
    "stage-0",
    ["env", {
      "modules": false,
      "targets": {
        "browsers": "> 1%",
        "ie": 11,
        "uglify": true
      },
      "useBuiltIns": true
    }]
  ],

  "plugins": [
    "syntax-dynamic-import",
    "transform-object-assign",
    "transform-object-rest-spread",
    ["transform-class-properties", {
      "spec": true
    }]
  ]
}
